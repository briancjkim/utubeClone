{
  "name": "utubeclone",
  "version": "1.0.0",
  "description": "youtube clone by nodejs",
  "main": "index.js",
  "scripts": {
    "dev:server": "nodemon --exec babel-node wow/init.js --delay 1 --ignore '.scss' --ignore 'static' ",
    "dev:assets": " webpack -w --config wow/webpack.config.js  WEBPACK_ENV=development",
    "build:assets": " webpack --config wow/webpack.config.js WEBPACK_ENV=production ",
    "build:server": "babel wow --out-dir build --ignore 'wow/assets','wow/static','wow/webpack.config.js'",
    "copyAll": "cp -R wow/static build && cp -R wow/views build",
    "build": "npm run build:server && npm run build:assets && npm run copyAll",
    "prebuild": "rm -rf build",
    "start": " PRODUCTION=true node build/init.js ",
    "prestart": "npm run build"
  },
  "author": "Brian chanjong Kim",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/polyfill": "^7.4.0",
    "@babel/preset-env": "^7.4.1",
    "autoprefixer": "^9.4.9",
    "aws-sdk": "^2.424.0",
    "axios": "^0.18.0",
    "babel-loader": "^8.0.5",
    "body-parser": "^1.18.3",
    "connect-mongo": "^2.0.3",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "get-blob-duration": "^1.0.1",
    "helmet": "^3.15.0",
    "mongoose": "^5.4.9",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "multer-s3": "^2.9.0",
    "node-sass": "^4.11.0",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-github": "^1.1.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "pug": "^2.0.3",
    "sass-loader": "^7.1.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "devDependencies": {
    "eslint": "^5.15.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "nodemon": "^1.18.9"
  },
  "engines": {
    "node": "^10.15.0",
    "npm": "^6.5.0"
  }
}
